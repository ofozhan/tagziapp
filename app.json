{
  "expo": {
    "name": "Tagzi",
    "slug": "tagzi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "sdkVersion": "51.0.0",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e272e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ofozhan.tagzi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1e272e"
      },
      "package": "com.ofozhan.tagzi"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "appDelegate": {
              "language": "objc"
            }
          }
        }
      ],
      [
        "expo-maps",
        {
          "ios": {
            "apiKey": "Maps_API_KEY_PLACEHOLDER"
          },
          "android": {
            "apiKey": "Maps_API_KEY_PLACEHOLDER"
          }
        }
      ]
    ]
  }
}